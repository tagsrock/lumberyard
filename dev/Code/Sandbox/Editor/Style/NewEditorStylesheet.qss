}
AzToolsFramework--PropertyAssetCtrl QLabel[DropHighlight="true"]
{
    background-color: @MenuItemSelectedColor;
}

/* Style for properties that fail validation */
AzToolsFramework--PropertyEntityIdCtrl[MismatchedServices="true"] AzToolsFramework--EntityIdQLabel
{
    font-family: "Open Sans Extrabold";
