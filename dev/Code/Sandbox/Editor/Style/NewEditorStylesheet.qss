/* ============================================================= */
/* Global styles                                                 */
/* ============================================================= */
*
{
	background-color: @WindowBackgroundColor;
	color:  @TextColor;
}

QMainWindow, QDialog
{
	color: @TextColor;
	background-color: @WindowBackgroundColor;
}

QDockWidget
{
	color: @DockWidgetTextColor;
