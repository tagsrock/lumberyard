/* Style for pulse highlight widgets (outliner highlight, etc) */
AzToolsFramework--OutlinerWidget QWidget[PulseHighlight="true"]
{
    background-color: @MenuItemSelectedColor;
}

AzToolsFramework--OutlinerWidget QLineEdit
{
